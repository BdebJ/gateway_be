{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug TypeScript",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "runtimeExecutable": "ts-node",
      "program": "${workspaceFolder}/src/server.ts",
      "sourceMaps": true,
      "envFile": "${workspaceFolder}/.env",
      "env": {
        "NODE_ENV": "development"
      },
    }
  ]
}